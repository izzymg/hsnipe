    <div class="gf-block-content globo-selected-items">
        <button onclick="clearAllFilter()" class="clear-refinements visible-sm">{{ translation.filter.clear_all | default: "Clear all"}}</button>
        {% if selectedItems %}
        {% for selectedItem in selectedItems %}
