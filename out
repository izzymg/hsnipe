                        <script>
                          function logMessage(message) {
                            console.log(`[Delivery Message Script]: ${message}`);
                          }

                          let debounceTimeout;

                          function updateDeliveryMessages() {
                            try {
                              logMessage('Updating delivery messages');
                              var currentTime = new Date();
                              var currentDay = currentTime.getDay();
                              var currentHour = currentTime.getHours();
                              var currentMinute = currentTime.getMinutes();
                              var isWeekend = (currentDay === 0 || currentDay === 6);

                              var cutoffTime = new Date();
                              cutoffTime.setHours(16, 0, 0, 0);
                              var isBeforeCutoff = (currentHour < 16 || (currentHour === 16 && currentMinute === 0));
                              var shipsToday = !isWeekend && isBeforeCutoff;
                              var shipsTomorrow = !isWeekend && !isBeforeCutoff && currentDay !== 5;

                              // Select all delivery message elements
                              var deliveryMessages = document.querySelectorAll(".delivery-message");

                              logMessage(`Found ${deliveryMessages.length} delivery message elements`);

                              // Iterate over each delivery message element
                              deliveryMessages.forEach(function (deliveryMessage) {
                                if (isWeekend) {
                                  deliveryMessage.innerHTML = " - Ships Monday";
                                } else if (shipsToday) {
                                  deliveryMessage.innerHTML = " - Ships Today!";
                                } else if (shipsTomorrow) {
                                  deliveryMessage.innerHTML = " - Ships tomorrow";
                                } else {
                                  deliveryMessage.innerHTML = " - Ships Monday";
                                }
                                logMessage(`Updated delivery message for element ID ${deliveryMessage.id}`);
                              });
                            } catch (error) {
                              logMessage(`Error updating delivery messages: ${error.message}`);
                            }
                          }

                          let observer;

                          function observeMutations() {
                            try {
                              if (observer) {
                                observer.disconnect();
                                logMessage('Existing MutationObserver disconnected');
                              }

                              var targetNodes = document.querySelectorAll(
                                '.container.relative, ' +
                                '#shopify-section-template--16180912521331__recommendations, ' +
                                '#shopify-section-template--16180912521331__recently_viewed_3L88f9, ' +
                                '#shopify-section-template--16180912521331__featured_collection_egVT3X, ' +
                                '#shopify-section-template--15660905496691__recommendations, ' +
                                '#shopify-section-sections--15660905726067__cart-drawer, ' +
                                '#shopify-section-sections--15660905726067__cart-drawer, ' +
                                'product-recommendations .container'
                              );
                              logMessage(`Found ${targetNodes.length} target nodes for mutation observer`);

                              if (!targetNodes.length) {
                                logMessage('Target nodes for mutation observer not found');
                                return;
                              }

                              var config = { childList: true, subtree: true, attributes: true, attributeFilter: ['class'] };

                              var callback = function (mutationsList) {
                                clearTimeout(debounceTimeout);
                                debounceTimeout = setTimeout(() => {
                                  logMessage('DOM mutation observed, updating delivery messages');
                                  if (observer) observer.disconnect(); // Pause the observer
                                  updateDeliveryMessages();
                                  bindEventListeners(); // Rebind event listeners after mutation
                                  if (observer) observeMutations(); // Resume the observer
                                }, 500); // Adjusted debounce timeout to 500 ms
                              };

                              observer = new MutationObserver(callback);
                              targetNodes.forEach(node => observer.observe(node, config));
                              logMessage('New MutationObserver initialized');
                            } catch (error) {
                              logMessage(`Error initializing MutationObserver: ${error.message}`);
                            }
                          }

                          function handleFilterChange() {
                            try {
                              logMessage('Filter changed, updating delivery messages');
                              setTimeout(function () {
                                if (observer) observer.disconnect(); // Pause the observer
                                updateDeliveryMessages();
                                observeMutations(); // Reset observer after update
                              }, 2000); // Adjusted timeout to 2000 ms
                            } catch (error) {
                              logMessage(`Error handling filter change: ${error.message}`);
                            }
                          }

                          function handleLoadMoreButtonClick() {
                            try {
                              logMessage('Load more button clicked, updating delivery messages');
                              setTimeout(function () {
                                if (observer) observer.disconnect(); // Pause the observer
                                updateDeliveryMessages();
                                observeMutations(); // Reset observer after update
                              }, 2000); // Adjusted timeout to 2000 ms
                            } catch (error) {
                              logMessage(`Error handling load more button click: ${error.message}`);
                            }
                          }

                          function bindEventListeners() {
                            try {
                              // Add event listener to filter elements
                              document.querySelectorAll('.facets__filters input[type="checkbox"], .facets__filters input[type="radio"], .facets__filters select, .facets__filters input[type="number"], .facets__filters input[type="range"]').forEach(function (filterElement) {
                                filterElement.removeEventListener('change', handleFilterChange);  // Remove existing listener
                                filterElement.addEventListener('change', handleFilterChange);  // Add new listener
                              });

                              document.querySelectorAll('.facets__filters button, .facets__filters a').forEach(function (filterElement) {
                                filterElement.removeEventListener('click', handleFilterChange);  // Remove existing listener
                                filterElement.addEventListener('click', function () {
                                  setTimeout(handleFilterChange, 2000); // Adjusted timeout to 2000 ms
                                });
                              });

                              // Add event listener to the "Clear all" buttons
                              document.querySelectorAll('.js-clear-all').forEach(function (clearAllButton) {
                                clearAllButton.removeEventListener('click', handleFilterChange);  // Remove existing listener
                                clearAllButton.addEventListener('click', function (event) {
                                  event.preventDefault();  // Prevent default behavior
                                  logMessage('"Clear all" button clicked, updating delivery messages');
                                  setTimeout(function () {
                                    handleFilterChange();
                                    bindEventListeners();  // Rebind event listeners
                                  }, 2000);  // Adjusted timeout to 2000 ms
                                });
                              });

                              // Add event listener to the "Clear" buttons
                              document.querySelectorAll('.js-clear-filter').forEach(function (clearButton) {
                                clearButton.removeEventListener('click', handleFilterChange);  // Remove existing listener
                                clearButton.addEventListener('click', function (event) {
                                  event.preventDefault();  // Prevent default behavior
                                  logMessage('"Clear" button clicked, updating delivery messages');
                                  setTimeout(function () {
                                    handleFilterChange();
                                    bindEventListeners();  // Rebind event listeners
                                  }, 2000);  // Adjusted timeout to 2000 ms
                                });
                              });

                              // Add event listener to filter options
                              document.querySelectorAll('.facets__filters label').forEach(function (filterOption) {
                                filterOption.removeEventListener('click', handleFilterChange);  // Remove existing listener
                                filterOption.addEventListener('click', function () {
                                  setTimeout(handleFilterChange, 2000);  // Adjusted timeout to 2000 ms
                                });
                              });

                              logMessage('Event listeners bound');
                            } catch (error) {
                              logMessage(`Error binding event listeners: ${error.message}`);
                            }
                          }

                          // Initialize all event listeners
                          function initializeEventListeners() {
                            observeMutations();
                            bindEventListeners();
                          }

                          // Run the function on page load
                          document.addEventListener("DOMContentLoaded", function () {
                            logMessage('Page loaded, initializing scripts');
                            setTimeout(function () {
                              updateDeliveryMessages();
                              initializeEventListeners();
                            }, 2000);  // Delay the initialization by 2000 ms

                            // Add event listener to the "load more" button
                            document.addEventListener('click', function (event) {
                              if (event.target && event.target.classList.contains('js-pagination-load-more')) {
                                handleLoadMoreButtonClick();
                              }
                            });

                            // Add event listener to the "Clear all" buttons
                            document.addEventListener('click', function (event) {
                              if (event.target && event.target.classList.contains('js-clear-all')) {
                                event.preventDefault();  // Prevent default behavior
                                logMessage('"Clear all" button clicked');
                                handleFilterChange();
                              }
                            });

                            // Add event listener to the "Clear" buttons
                            document.addEventListener('click', function (event) {
                              if (event.target && event.target.classList.contains('js-clear-filter')) {
                                event.preventDefault();  // Prevent default behavior
                                logMessage('"Clear" button clicked');
                                handleFilterChange();
                              }
                            });

                            // Add event listener to filter options
                            document.addEventListener('click', function (event) {
                              if (event.target && event.target.closest('.facets__filters label')) {
                                handleFilterChange();
                              }
                            });
                          });
                        </script>
