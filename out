                 <span class="product-label product-label--pc">
                 {{ product.metafields.custom.ramcapacity }}
                 </span>
                </div>
               {%- endif -%}
  
                {%- if product.metafields.custom.laptopcpuname != blank -%}
                  <div class="product-label-wrapper flex" {{ block.shopify_attributes }}>
                     <span class="product-label product-label--pc">
                     {{ product.metafields.custom.laptopcpuname }}
                     </span>
                  </div>
               {%- endif -%}
               {%- if product.metafields.custom.laptopgpufamily != blank -%}
