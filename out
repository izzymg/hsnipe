            {% endif %}
            <div data-filter-type="{{filter.attribute | replace: 'option:', ''}}" data-filter-id="{{filter.id}}" class="gf-option-block {{filter.class}}{% if filter.is_collapse %} is-collapsed{% endif %}">
            <div class="gf-block-title">
                <div class="h3">
                <span>{{filterLabel}}</span>
